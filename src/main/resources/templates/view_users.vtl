#if($admin)
    <table class="table">
        <thead>
        <tr>
            <th>Gebruikersnaam</th>
            <th>Voornaam</th>
            <th>Achternaam</th>
        </tr>
        </thead>
        <tbody>

        #foreach ($u in $users)
            <tr>
                <td>$u.usernameName</td>
                <td>$u.userName</td>
                <td>$u.userSurname</td>
            </tr>
        #end

        </tbody>
    </table>
#else
#end